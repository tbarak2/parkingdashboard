<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Parking Dashboard</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
   
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @*@Scripts.Render("~/Scripts/angular-google-maps.js")*@
    @Scripts.Render("~/Scripts/app/app.js")
    @Scripts.Render("~/Scripts/app/services/gisSitesService.js")
    @Scripts.Render("~/Scripts/app/controller/gisSiteController.js")
    @RenderSection("scripts", required: false)
</head>
<body>
    <header>
        <div class="content-wrapper">
            <div class="float-left">
                <p class="site-title">@Html.ActionLink("Parking Dashboard", "Index", "SiteData")</p>
            </div>
        </div>
    </header>
    <div id="body" ng-app="GisSites">
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Table", "Index", "SiteData")</li>
                    <li>@Html.ActionLink("Gis", "Index", "GisSitesData")</li>
                    @*<li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                </ul>
            </div>
        </div>
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
    <footer>
        <div class="content-wrapper">
            <div class="float-left">
                <p>&copy; @DateTime.Now.Year - Parking Dashboard</p>
            </div>
        </div>
    </footer>
</body>
</html>






